extends ../layouts/layout-desktop

block content
	.page-header
		h1 Award
			small  browser
	.row
		.span12
			.awards(data-bind='foreach: awards')
				.rounded-box.grey-box
					img.pull-left(data-bind='attr: { src: image }', style='padding-right: 15px;')
					h3(data-bind='text: name')
					em.muted(data-bind='text: desc')
					h1(data-bind='text: worth')

append scripts
	script(src='/desktop/js/awards.knockout.js')
